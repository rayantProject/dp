    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>blur_service_dotnet</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Start.cmd" />
  </ItemGroup>

  <ItemGroup>
